{
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "indent": ["error", 4],
        "prefer-const": 0,
        "no-unused-vars": [2, {"vars": "all", "args": "none"}],
        "semi": [2, "never"]
    },
    "extends": "eslint-config-airbnb"
}
